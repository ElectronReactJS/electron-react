{
  "name": "electron-react-ts",
  "productName": "electron-react-ts",
  "version": "1.0.0",
  "description": "My Electron React TypeScript application",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start && npm run commit:start && npm run format && npm run commit:format",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "prettier --write --jsx-single-quote '**/*.{js,jsx,ts,tsx,json,css,html}' --log-level warn",
    "clean": "npm cache clean --force && rm -rf 'node_modules' '.webpack' && npm install -y",
    "commit:auto": "git add . && git commit -m \"path: commit by npm run commit\"",
    "commit:start": "git add . && git commit -m \"path: auto commit after npm start\"",
    "commit:format": "git add . && git commit -m \"path: commit by npm run format\""
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/maker-deb": "^7.3.0",
    "@electron-forge/maker-rpm": "^7.3.0",
    "@electron-forge/maker-squirrel": "^7.3.0",
    "@electron-forge/maker-zip": "^7.3.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.0",
    "@electron-forge/plugin-fuses": "^7.3.0",
    "@electron-forge/plugin-webpack": "^7.3.0",
    "@electron/fuses": "^1.7.0",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "css-loader": "^6.10.0",
    "electron": "29.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "node-loader": "^2.0.0",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4"
  },
  "keywords": [],
  "author": {
    "name": "Malnati",
    "email": "ricardomalnati@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.15.10",
    "@mui/system": "^5.14.17",
    "@mui/x-data-grid": "^6.18.1",
    "@types/react-router-dom": "^5.3.3",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0"
  }
}
